{
  "name": "stylecreator",
  "version": "1.0.0",
  "description": "Style guides creator",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --config ./config/webpack.config.js --progress --profile --colors",
    "postinstall": "npm run build",
    "start": "node server.js",
    "publish": "./bin/deploy.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stylecreator/stylecreator.github.io.git"
  },
  "keywords": [
    "style",
    "style guide"
  ],
  "author": "Sara, Hector, Sofia",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "basic-auth-connect": "^1.0.0",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.23.1",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "jade": "^1.11.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.13.1",
    "morgan": "^1.7.0",
    "node-sass": "^3.8.0",
    "node.normalize.scss": "^3.0.3",
    "normalize.css": "^4.2.0",
    "pretty-error": "^2.0.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-draggable": "^2.1.2",
    "react-google-maps": "^4.11.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.0",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "whatwg-fetch": "^1.0.0",
    "xo": "^0.16.0"
  },
  "engines": {
    "node": "5.10.x",
    "npm": "3.8.x"
  },
  "devDependencies": {},
  "xo": {
    "esnext": true,
    "space": 2,
    "ignores": [
      "app/**"
    ],
    "rules": {
      "space-before-function-paren": [
        "error",
        {
          "anonymous": "never",
          "named": "never"
        }
      ],
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ]
    }
  },
  "bugs": {
    "url": "https://github.com/stylecreator/stylecreator.github.io/issues"
  },
  "homepage": "https://github.com/stylecreator/stylecreator.github.io#readme"
}
